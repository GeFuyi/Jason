  data() {\r\n    return {\r\n      form: {\r\n        username: '',\r\n        password: ''\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    submit() {\r\n      axios.post('http://localhost:8090/user/login', this.form)\r\n          .then(res => {\r\n            if (res.data === '登录成功') {\r\n              this.$message.success(res.data)\r\n              this.$router.push('/') // 登录成功跳转首页\r\n            } else {\r\n              this.$message.error(res.data)\r\n            }\r\n          })\r\n          .catch(err => {\r\n            this.$message.error('登录请求失败')\r\n            console.error(err)\r\n          })\r\n    },\r\n    goRegister() {\r\n      this.$router.push('/register')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.auth-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  height: 100vh; /* 垂直居中 */\r\n  background: #f5f5f5;\r\n}\r\n\r\n.auth-card {\r\n  width: 400px;\r\n  padding: 30px;\r\n}\r\n</style>\r\n"],"mappings":";AAwBA,OAAOA,KAAI,MAAO,OAAM;AACxB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE;MACZ;IACF;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,MAAMA,CAAA,EAAG;MACPP,KAAK,CAACQ,IAAI,CAAC,kCAAkC,EAAE,IAAI,CAACL,IAAI,EACnDM,IAAI,CAACC,GAAE,IAAK;QACX,IAAIA,GAAG,CAACR,IAAG,KAAM,MAAM,EAAE;UACvB,IAAI,CAACS,QAAQ,CAACC,OAAO,CAACF,GAAG,CAACR,IAAI;UAC9B,IAAI,CAACW,OAAO,CAACC,IAAI,CAAC,GAAG,GAAE;QACzB,OAAO;UACL,IAAI,CAACH,QAAQ,CAACI,KAAK,CAACL,GAAG,CAACR,IAAI;QAC9B;MACF,CAAC,EACAc,KAAK,CAACC,GAAE,IAAK;QACZ,IAAI,CAACN,QAAQ,CAACI,KAAK,CAAC,QAAQ;QAC5BG,OAAO,CAACH,KAAK,CAACE,GAAG;MACnB,CAAC;IACP,CAAC;IACDE,UAAUA,CAAA,EAAG;MACX,IAAI,CAACN,OAAO,CAACC,IAAI,CAAC,WAAW;IAC/B;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}