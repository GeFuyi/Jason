{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport egg from '@/utils/egg';\nexport default {\n  name: 'UserRegister',\n  data() {\n    return {\n      form: {\n        username: '',\n        email: '',\n        password: '',\n        age: 18,\n        // 默认值\n        gender: 1 // 默认男\n      }\n    };\n  },\n  methods: {\n    submit() {\n      // 如果用户没有选择性别或年龄为空，也设置默认值\n      if (this.form.age === null) this.form.age = 18;\n      if (this.form.gender === null) this.form.gender = 1;\n      egg.post('/user/register', this.form).then(res => {\n        console.log('注册返回', res.data);\n        if (res.data.success) {\n          this.$message.success(res.data.message);\n          this.$router.push('/login');\n        } else {\n          this.$message.error(res.data.message);\n        }\n      }).catch(err => {\n        this.$message.error('注册请求失败');\n        console.error(err);\n      });\n    },\n    goLogin() {\n      this.$router.push('/login');\n    }\n  }\n};","map":{"version":3,"names":["egg","name","data","form","username","email","password","age","gender","methods","submit","post","then","res","console","log","success","$message","message","$router","push","error","catch","err","goLogin"],"sources":["D:\\Final\\springboot_egg\\egg-web\\src\\user\\Register.vue"],"sourcesContent":["<template>\r\n  <div class=\"auth-container\">\r\n    <el-card class=\"auth-card\">\r\n      <h2 style=\"text-align:center;margin-bottom:20px\">注册</h2>\r\n      <el-form :model=\"form\" ref=\"registerForm\" label-width=\"80px\">\r\n        <el-form-item label=\"用户名\">\r\n          <el-input v-model=\"form.username\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"邮箱\">\r\n          <el-input v-model=\"form.email\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"密码\">\r\n          <el-input type=\"password\" v-model=\"form.password\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"年龄\">\r\n          <el-input type=\"number\" v-model=\"form.age\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"性别\">\r\n          <el-radio-group v-model=\"form.gender\">\r\n            <el-radio :label=\"1\">男</el-radio>\r\n            <el-radio :label=\"0\">女</el-radio>\r\n          </el-radio-group>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"submit\" style=\"width:100%\">注册</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div style=\"text-align:center\">\r\n        <span>已有账号？</span>\r\n        <el-link type=\"primary\" @click=\"goLogin\">去登录</el-link>\r\n      </div>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport egg from '@/utils/egg'\r\nexport default {\r\n  name: 'UserRegister',\r\n  data() {\r\n    return {\r\n      form: {\r\n        username: '',\r\n        email: '',\r\n        password: '',\r\n        age: 18,       // 默认值\r\n        gender: 1      // 默认男\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    submit() {\r\n      // 如果用户没有选择性别或年龄为空，也设置默认值\r\n      if (this.form.age === null) this.form.age = 18\r\n      if (this.form.gender === null) this.form.gender = 1\r\n\r\n      egg.post('/user/register', this.form)\r\n          .then(res => {\r\n            console.log('注册返回', res.data)\r\n            if (res.data.success) {\r\n              this.$message.success(res.data.message)\r\n              this.$router.push('/login')\r\n            } else {\r\n              this.$message.error(res.data.message)\r\n            }\r\n          })\r\n          .catch(err => {\r\n            this.$message.error('注册请求失败')\r\n            console.error(err)\r\n          })\r\n    },\r\n\r\n    goLogin() {\r\n      this.$router.push('/login')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.auth-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  height: 100vh;\r\n  background: #f5f5f5;\r\n}\r\n\r\n.auth-card {\r\n  width: 400px;\r\n  padding: 30px;\r\n}\r\n</style>\r\n"],"mappings":";AAoCA,OAAOA,GAAE,MAAO,aAAY;AAC5B,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,GAAG,EAAE,EAAE;QAAQ;QACfC,MAAM,EAAE,EAAO;MACjB;IACF;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,MAAMA,CAAA,EAAG;MACP;MACA,IAAI,IAAI,CAACP,IAAI,CAACI,GAAE,KAAM,IAAI,EAAE,IAAI,CAACJ,IAAI,CAACI,GAAE,GAAI,EAAC;MAC7C,IAAI,IAAI,CAACJ,IAAI,CAACK,MAAK,KAAM,IAAI,EAAE,IAAI,CAACL,IAAI,CAACK,MAAK,GAAI;MAElDR,GAAG,CAACW,IAAI,CAAC,gBAAgB,EAAE,IAAI,CAACR,IAAI,EAC/BS,IAAI,CAACC,GAAE,IAAK;QACXC,OAAO,CAACC,GAAG,CAAC,MAAM,EAAEF,GAAG,CAACX,IAAI;QAC5B,IAAIW,GAAG,CAACX,IAAI,CAACc,OAAO,EAAE;UACpB,IAAI,CAACC,QAAQ,CAACD,OAAO,CAACH,GAAG,CAACX,IAAI,CAACgB,OAAO;UACtC,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ;QAC5B,OAAO;UACL,IAAI,CAACH,QAAQ,CAACI,KAAK,CAACR,GAAG,CAACX,IAAI,CAACgB,OAAO;QACtC;MACF,CAAC,EACAI,KAAK,CAACC,GAAE,IAAK;QACZ,IAAI,CAACN,QAAQ,CAACI,KAAK,CAAC,QAAQ;QAC5BP,OAAO,CAACO,KAAK,CAACE,GAAG;MACnB,CAAC;IACP,CAAC;IAEDC,OAAOA,CAAA,EAAG;MACR,IAAI,CAACL,OAAO,CAACC,IAAI,CAAC,QAAQ;IAC5B;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}